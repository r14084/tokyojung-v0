{
  "version": 2,
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.0.27"
    },
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.27"
    }
  },
  "buildCommand": "bash build.sh",
  "nodeVersion": "20.x",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/staff(.*)",
      "dest": "/staff$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}